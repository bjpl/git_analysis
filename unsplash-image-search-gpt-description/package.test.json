{\n  \"name\": \"vocablens-pwa\",\n  \"version\": \"1.0.0\",\n  \"type\": \"module\",\n  \"description\": \"VocabLens - Visual Vocabulary Learning PWA with comprehensive testing infrastructure\",\n  \"scripts\": {\n    \"dev\": \"vite\",\n    \"build\": \"tsc && vite build\",\n    \"preview\": \"vite preview\",\n    \"lint\": \"eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0\",\n    \"lint:fix\": \"eslint . --ext ts,tsx --fix\",\n    \"typecheck\": \"tsc --noEmit\",\n    \"format\": \"prettier --write \\\"src/**/*.{ts,tsx,js,jsx,css,md,json}\\\"\",\n    \"format:check\": \"prettier --check \\\"src/**/*.{ts,tsx,js,jsx,css,md,json}\\\"\",\n    \n    \"test\": \"vitest\",\n    \"test:ui\": \"vitest --ui\",\n    \"test:run\": \"vitest run\",\n    \"test:watch\": \"vitest --watch\",\n    \"test:coverage\": \"vitest run --coverage\",\n    \"test:unit\": \"vitest run --config vitest.config.unit.ts\",\n    \"test:integration\": \"vitest run --config vitest.config.integration.ts\",\n    \n    \"test:e2e\": \"playwright test\",\n    \"test:e2e:ui\": \"playwright test --ui\",\n    \"test:e2e:debug\": \"playwright test --debug\",\n    \"test:e2e:headed\": \"playwright test --headed\",\n    \"test:e2e:chromium\": \"playwright test --project=chromium\",\n    \"test:e2e:firefox\": \"playwright test --project=firefox\",\n    \"test:e2e:webkit\": \"playwright test --project=webkit\",\n    \"test:e2e:mobile\": \"playwright test --project=\\\"Mobile Chrome\\\" --project=\\\"Mobile Safari\\\"\",\n    \"test:e2e:accessibility\": \"playwright test --project=accessibility\",\n    \n    \"test:performance\": \"playwright test tests/performance --project=chromium\",\n    \"test:lighthouse\": \"lhci autorun\",\n    \"test:lighthouse:collect\": \"lhci collect\",\n    \"test:lighthouse:upload\": \"lhci upload\",\n    \n    \"test:all\": \"npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance\",\n    \"test:ci\": \"npm run test:coverage && npm run test:e2e && npm run test:lighthouse\",\n    \n    \"playwright:install\": \"playwright install\",\n    \"playwright:install-deps\": \"playwright install-deps\",\n    \"playwright:codegen\": \"playwright codegen http://localhost:3000\",\n    \n    \"serve:test\": \"vite build && vite preview --port 4173\",\n    \"clean\": \"rm -rf dist coverage playwright-report test-results .nyc_output\",\n    \"clean:node_modules\": \"rm -rf node_modules && npm install\",\n    \n    \"validate\": \"npm run typecheck && npm run lint && npm run format:check && npm run test:coverage\",\n    \"prepare\": \"husky install\"\n  },\n  \"dependencies\": {\n    \"@heroicons/react\": \"^2.0.18\",\n    \"@supabase/supabase-js\": \"^2.39.3\",\n    \"@tanstack/react-query\": \"^5.17.19\",\n    \"react\": \"^18.2.0\",\n    \"react-dom\": \"^18.2.0\",\n    \"react-hot-toast\": \"^2.4.1\",\n    \"react-router-dom\": \"^6.21.3\",\n    \"react-window\": \"^1.8.8\",\n    \"react-window-infinite-loader\": \"^1.0.9\",\n    \"zustand\": \"^4.4.7\"\n  },\n  \"devDependencies\": {\n    \"@playwright/test\": \"^1.40.1\",\n    \"@testing-library/jest-dom\": \"^6.2.0\",\n    \"@testing-library/react\": \"^14.1.2\",\n    \"@testing-library/user-event\": \"^14.5.2\",\n    \"@types/node\": \"^20.10.8\",\n    \"@types/react\": \"^18.2.48\",\n    \"@types/react-dom\": \"^18.2.18\",\n    \"@types/react-window\": \"^1.8.8\",\n    \"@types/react-window-infinite-loader\": \"^1.0.9\",\n    \"@typescript-eslint/eslint-plugin\": \"^6.19.1\",\n    \"@typescript-eslint/parser\": \"^6.19.1\",\n    \"@vitejs/plugin-react\": \"^4.2.1\",\n    \"@vitest/coverage-v8\": \"^1.2.1\",\n    \"@vitest/ui\": \"^1.2.1\",\n    \"autoprefixer\": \"^10.4.16\",\n    \"eslint\": \"^8.56.0\",\n    \"eslint-plugin-react-hooks\": \"^4.6.0\",\n    \"eslint-plugin-react-refresh\": \"^0.4.5\",\n    \"happy-dom\": \"^13.3.1\",\n    \"husky\": \"^8.0.3\",\n    \"jsdom\": \"^24.0.0\",\n    \"@lhci/cli\": \"^0.12.0\",\n    \"lint-staged\": \"^15.2.0\",\n    \"msw\": \"^2.0.11\",\n    \"postcss\": \"^8.4.33\",\n    \"prettier\": \"^3.2.4\",\n    \"tailwindcss\": \"^3.4.1\",\n    \"typescript\": \"^5.3.3\",\n    \"vite\": \"^5.0.11\",\n    \"vite-plugin-pwa\": \"^0.17.4\",\n    \"vitest\": \"^1.2.1\",\n    \"claude-flow\": \"^2.0.0-alpha.91\"\n  },\n  \"lint-staged\": {\n    \"*.{ts,tsx}\": [\n      \"eslint --fix\",\n      \"prettier --write\"\n    ],\n    \"*.{js,jsx,css,md,json}\": [\n      \"prettier --write\"\n    ]\n  },\n  \"msw\": {\n    \"workerDirectory\": \"public\"\n  },\n  \"engines\": {\n    \"node\": \">=18.0.0\",\n    \"npm\": \">=9.0.0\"\n  },\n  \"keywords\": [\n    \"pwa\",\n    \"react\",\n    \"typescript\",\n    \"vocabulary\",\n    \"learning\",\n    \"ai\",\n    \"unsplash\",\n    \"openai\",\n    \"supabase\",\n    \"testing\",\n    \"vitest\",\n    \"playwright\",\n    \"lighthouse\"\n  ],\n  \"author\": \"\",\n  \"license\": \"MIT\",\n  \"repository\": {\n    \"type\": \"git\",\n    \"url\": \"git+https://github.com/your-username/vocablens-pwa.git\"\n  },\n  \"bugs\": {\n    \"url\": \"https://github.com/your-username/vocablens-pwa/issues\"\n  },\n  \"homepage\": \"https://github.com/your-username/vocablens-pwa#readme\"\n}"