{
  "Main": {
    "name": {
      "type": "Text",
      "config": {
        "label": "Project Name",
        "placeholder": "e.g., Intelligent Document Processor"
      }
    },
    "slug": {
      "type": "UID",
      "config": {
        "label": "URL Slug",
        "placeholder": "project-url-slug"
      }
    },
    "project_type": {
      "type": "Select",
      "config": {
        "label": "Project Type",
        "options": [
          "Machine Learning",
          "Natural Language Processing",
          "Computer Vision",
          "Robotics",
          "Data Science",
          "AI Application",
          "Research Project",
          "Open Source Tool",
          "Commercial Product"
        ]
      }
    },
    "status": {
      "type": "Select",
      "config": {
        "label": "Project Status",
        "options": [
          "Completed",
          "In Progress",
          "Prototype",
          "Concept",
          "Deprecated"
        ],
        "default_value": "Completed"
      }
    },
    "featured_image": {
      "type": "Image",
      "config": {
        "label": "Featured Image",
        "constraint": {
          "width": 1200,
          "height": 675
        },
        "thumbnails": [
          {
            "name": "card",
            "width": 600,
            "height": 400
          },
          {
            "name": "small",
            "width": 300,
            "height": 200
          }
        ]
      }
    }
  },
  "Problem & Solution": {
    "problem": {
      "type": "StructuredText",
      "config": {
        "label": "Problem Statement",
        "placeholder": "What problem does this project solve?",
        "allowTargetBlank": true,
        "multi": "paragraph,preformatted,heading3,heading4,heading5,heading6,strong,em,hyperlink,list-item,o-list-item"
      }
    },
    "solution": {
      "type": "StructuredText",
      "config": {
        "label": "Solution Description",
        "placeholder": "How does your AI solution work?",
        "allowTargetBlank": true,
        "multi": "paragraph,preformatted,heading3,heading4,heading5,heading6,strong,em,hyperlink,list-item,o-list-item"
      }
    },
    "approach": {
      "type": "StructuredText",
      "config": {
        "label": "Technical Approach",
        "placeholder": "Detailed technical methodology and approach",
        "allowTargetBlank": true,
        "multi": "paragraph,preformatted,heading3,heading4,heading5,heading6,strong,em,hyperlink,list-item,o-list-item"
      }
    }
  },
  "Technical Details": {
    "tech_stack": {
      "type": "Group",
      "config": {
        "label": "Technology Stack",
        "repeat": true,
        "fields": {
          "category": {
            "type": "Select",
            "config": {
              "label": "Category",
              "options": [
                "Programming Language",
                "Framework/Library",
                "Database",
                "Cloud Platform",
                "AI/ML Tool",
                "Development Tool",
                "API/Service",
                "Infrastructure"
              ]
            }
          },
          "technology": {
            "type": "Text",
            "config": {
              "label": "Technology",
              "placeholder": "e.g., Python, TensorFlow, AWS"
            }
          },
          "version": {
            "type": "Text",
            "config": {
              "label": "Version",
              "placeholder": "Version number (optional)"
            }
          },
          "purpose": {
            "type": "Text",
            "config": {
              "label": "Purpose",
              "placeholder": "What role does this technology play?"
            }
          }
        }
      }
    },
    "algorithms": {
      "type": "Group",
      "config": {
        "label": "Algorithms & Models",
        "repeat": true,
        "fields": {
          "name": {
            "type": "Text",
            "config": {
              "label": "Algorithm/Model Name",
              "placeholder": "e.g., BERT, Random Forest, CNN"
            }
          },
          "type": {
            "type": "Select",
            "config": {
              "label": "Type",
              "options": [
                "Neural Network",
                "Machine Learning",
                "Deep Learning",
                "NLP Model",
                "Computer Vision",
                "Reinforcement Learning",
                "Statistical Model",
                "Custom Algorithm"
              ]
            }
          },
          "description": {
            "type": "Text",
            "config": {
              "label": "Description",
              "placeholder": "Brief description of usage"
            }
          }
        }
      }
    },
    "data_sources": {
      "type": "Group",
      "config": {
        "label": "Data Sources",
        "repeat": true,
        "fields": {
          "source_name": {
            "type": "Text",
            "config": {
              "label": "Source Name",
              "placeholder": "Name of the data source"
            }
          },
          "data_type": {
            "type": "Select",
            "config": {
              "label": "Data Type",
              "options": [
                "Text",
                "Images",
                "Audio",
                "Video",
                "Structured Data",
                "Time Series",
                "Graph Data",
                "Sensor Data"
              ]
            }
          },
          "size": {
            "type": "Text",
            "config": {
              "label": "Dataset Size",
              "placeholder": "e.g., 1M records, 10GB, 50K images"
            }
          },
          "description": {
            "type": "Text",
            "config": {
              "label": "Description",
              "placeholder": "What this data represents"
            }
          }
        }
      }
    }
  },
  "Results & Impact": {
    "results": {
      "type": "Group",
      "config": {
        "label": "Results & Metrics",
        "repeat": true,
        "fields": {
          "metric_name": {
            "type": "Text",
            "config": {
              "label": "Metric Name",
              "placeholder": "e.g., Accuracy, F1 Score, Processing Time"
            }
          },
          "value": {
            "type": "Text",
            "config": {
              "label": "Value",
              "placeholder": "e.g., 95.2%, 10x faster, $1M saved"
            }
          },
          "baseline": {
            "type": "Text",
            "config": {
              "label": "Baseline/Previous",
              "placeholder": "What was the previous performance?"
            }
          },
          "is_primary": {
            "type": "Boolean",
            "config": {
              "label": "Primary Metric",
              "default_value": false
            }
          }
        }
      }
    },
    "impact": {
      "type": "StructuredText",
      "config": {
        "label": "Impact & Benefits",
        "placeholder": "What was the real-world impact of this project?",
        "allowTargetBlank": true,
        "multi": "paragraph,preformatted,heading3,heading4,heading5,heading6,strong,em,hyperlink,list-item,o-list-item"
      }
    },
    "challenges": {
      "type": "StructuredText",
      "config": {
        "label": "Challenges & Solutions",
        "placeholder": "What challenges did you face and how did you solve them?",
        "allowTargetBlank": true,
        "multi": "paragraph,preformatted,heading3,heading4,heading5,heading6,strong,em,hyperlink,list-item,o-list-item"
      }
    }
  },
  "Media & Links": {
    "screenshots": {
      "type": "Group",
      "config": {
        "label": "Screenshots",
        "repeat": true,
        "fields": {
          "image": {
            "type": "Image",
            "config": {
              "label": "Screenshot",
              "constraint": {
                "width": 1200,
                "height": 800
              },
              "thumbnails": [
                {
                  "name": "preview",
                  "width": 600,
                  "height": 400
                }
              ]
            }
          },
          "caption": {
            "type": "Text",
            "config": {
              "label": "Caption",
              "placeholder": "Describe what this screenshot shows"
            }
          },
          "alt_text": {
            "type": "Text",
            "config": {
              "label": "Alt Text",
              "placeholder": "Alternative text for accessibility"
            }
          }
        }
      }
    },
    "demo_link": {
      "type": "Link",
      "config": {
        "label": "Demo Link",
        "placeholder": "Link to live demo",
        "allowTargetBlank": true,
        "select": null
      }
    },
    "github_link": {
      "type": "Link",
      "config": {
        "label": "GitHub Repository",
        "placeholder": "Link to GitHub repo",
        "allowTargetBlank": true,
        "select": null
      }
    },
    "additional_links": {
      "type": "Group",
      "config": {
        "label": "Additional Links",
        "repeat": true,
        "fields": {
          "link_type": {
            "type": "Select",
            "config": {
              "label": "Link Type",
              "options": [
                "Documentation",
                "Blog Post",
                "Paper/Research",
                "Presentation",
                "Video Demo",
                "Dataset",
                "API",
                "Other"
              ]
            }
          },
          "url": {
            "type": "Link",
            "config": {
              "label": "URL",
              "allowTargetBlank": true
            }
          },
          "title": {
            "type": "Text",
            "config": {
              "label": "Link Title",
              "placeholder": "Descriptive title for the link"
            }
          }
        }
      }
    }
  },
  "Metadata": {
    "display_order": {
      "type": "Number",
      "config": {
        "label": "Display Order",
        "placeholder": "Lower numbers appear first"
      }
    },
    "is_featured": {
      "type": "Boolean",
      "config": {
        "label": "Featured Project",
        "default_value": false
      }
    },
    "tags": {
      "type": "Group",
      "config": {
        "label": "Tags",
        "repeat": true,
        "fields": {
          "tag": {
            "type": "Text",
            "config": {
              "label": "Tag",
              "placeholder": "e.g., NLP, Computer Vision, Production"
            }
          }
        }
      }
    },
    "completion_date": {
      "type": "Date",
      "config": {
        "label": "Completion Date"
      }
    },
    "duration": {
      "type": "Text",
      "config": {
        "label": "Project Duration",
        "placeholder": "e.g., 6 months, 2 years"
      }
    }
  }
}