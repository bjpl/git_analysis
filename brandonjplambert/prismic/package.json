{
  "name": "prismic-portfolio-integration",
  "version": "1.0.0",
  "description": "Professional portfolio Prismic CMS integration with TypeScript support",
  "main": "lib/client.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "prismic:setup": "prismic login && prismic ct --source ./schemas",
    "prismic:types": "prismic-types --output types/generated.ts",
    "validate-schemas": "node scripts/validate-schemas.js"
  },
  "keywords": [
    "prismic",
    "cms",
    "portfolio",
    "typescript",
    "nextjs",
    "react",
    "headless-cms"
  ],
  "author": "Your Name <your.email@domain.com>",
  "license": "MIT",
  "dependencies": {
    "@prismicio/client": "^7.0.0",
    "@prismicio/next": "^1.0.0",
    "@prismicio/helpers": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prismic-cli": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "next": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/prismic-portfolio-integration.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/prismic-portfolio-integration/issues"
  },
  "homepage": "https://github.com/your-username/prismic-portfolio-integration#readme",
  "files": [
    "schemas/",
    "types/",
    "lib/",
    "utils/",
    "docs/",
    "README.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}