# Corporate Intelligence Platform Environment Variables
# Copy this file to .env and update with your actual values

# Environment
ENVIRONMENT=development
DEBUG=false

# Security
SECRET_KEY=your-secret-key-here-generate-with-openssl-rand-hex-32

# Database
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=intel_user
POSTGRES_PASSWORD=your-secure-password
POSTGRES_DB=corporate_intel

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
REDIS_DB=0

# MinIO
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_USE_SSL=false
MINIO_BUCKET_DOCUMENTS=corporate-documents
MINIO_BUCKET_REPORTS=analysis-reports

# Prefect
PREFECT_API_URL=http://localhost:4200/api

# Ray
RAY_HEAD_ADDRESS=ray://localhost:10001

# External APIs
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-key
NEWSAPI_KEY=your-newsapi-key
CRUNCHBASE_API_KEY=your-crunchbase-key
GITHUB_TOKEN=your-github-token

# SEC EDGAR
SEC_USER_AGENT=Corporate Intel Platform/1.0 (your-email@example.com)

# OpenTelemetry
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_SERVICE_NAME=corporate-intel-api
OTEL_ENABLED=true

# Grafana
GRAFANA_PASSWORD=admin

# Superset
SUPERSET_SECRET_KEY=your-superset-secret-key

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PER_MINUTE=60

# Data Quality
DATA_QUALITY_ENABLED=true
ANOMALY_DETECTION_ENABLED=true

# Embeddings
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
EMBEDDING_DIMENSION=384
EMBEDDING_BATCH_SIZE=32

# Cache
CACHE_TTL_SECONDS=3600
CACHE_ENABLED=true

# Logging
LOG_LEVEL=INFO